  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "baseUrl": "src/"
  },
  "exclude": ["./node_modules/**/*"],
  "include": ["src"]
}
